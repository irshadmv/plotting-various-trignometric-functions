import numpy as np
import matplotlib.pyplot as plt
x=np.arange(0,10,0.01)
s=np.sin(x)
c=np.cos(x)
sh=np.sinh(x)
ch=np.cosh(x)
fig=plt.figure(figsize=(10,10))
plt.subplot(2,2,1)
plt.title('sin(x) vs x')
plt.xlabel('x')
plt.ylabel('sin(x)')
plt.plot(x,s)
plt.subplot(2,2,2)
plt.title('cos(x) vs x')
plt.xlabel('x')
plt.ylabel('cos(x)')
plt.plot(x,c)
plt.subplot(2,2,3)
plt.title('sinh(x) vs x')
plt.xlabel('x')
plt.ylabel('sinh(x)')
plt.plot(x,sh)
plt.subplot(2,2,4)
plt.title('cosh(x) vs x')
plt.xlabel('x')
plt.ylabel('cosh(x)')
plt.plot(x,ch)
plt.show()
